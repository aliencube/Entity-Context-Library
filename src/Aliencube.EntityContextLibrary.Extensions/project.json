{
  "version": "2.0.0-*",

  "title": "Entity Framework Context Library Extensions",
  "authors": [
    "justinyoo"
  ],

  "packOptions": {
    "owners": [
      "justinyoo"
    ],
    "licenseUrl": "https://github.com/aliencube/Entity-Context-Library/blob/master/LICENSE",
    "projectUrl": "https://github.com/aliencube/Entity-Context-Library",
    "summary": "Entity Framework Context Library (ECL) to provide DbContextFactory, UnitOfWork and UnitOfWorkManager using Entity Framework Core.",
    "tags": [
      "Aliencube",
      "Entity Framework",
      "EF",
      "DbContext"
    ],
    "requireLicenseAcceptance": true
  },

  "description": "Entity Framework Context Library (ECL) provides DbContextFactory to support mutliple DbContext instances within a UnitOfWork transaction scope through UnitOfWorkManager, using Entity Framework Core. This is an extension library of ECL",
  "copyright": "Aliencube Community. All rights reserved.",

  "frameworks": {
    "net451": {
      "dependencies": {
        "System.Runtime": "4.0.10.0"
      }
    },

    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      }
    }
  },

  "dependencies": {
    "Microsoft.EntityFrameworkCore.Tools": {
      "version": "1.0.0-preview2-final",
      "type": "build"
    }
  },

  "tools": {
    "Microsoft.EntityFrameworkCore.Tools": "1.0.0-preview2-final"
  },

  "buildOptions": {
    "define": [ "DEBUG", "TRACE" ],
    "xmlDoc": true
  }
}
